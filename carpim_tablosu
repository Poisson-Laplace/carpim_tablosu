#include <iostream>
using namespace std;
int main() {
    for (int i = 1; i <= 9; i++) {
        switch (i) {
        case 1:
            cout << "   " << i << "\t";
            break;
        default:
            cout << i << "\t";
            break;
        }  
    }
cout << "\n";
    for (int c = 1; c < 10; c++) {
        cout << c << "| ";
        for (int i = 1; i < 10; i++) {
            cout << i * c << '\t';
        }
        cout << endl;
    }
    return 0;
}
